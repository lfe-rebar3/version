{erl_opts, []}.

{deps, [
  {lfe, ".*", {git, "https://github.com/rvirding/lfe.git", {branch, "develop"}}},
  {lutil, ".*", {git, "https://github.com/lfex/lutil.git", {branch, "master"}}}
  {ltest, ".*", {git, "https://github.com/lfex/ltest.git", {branch, "master"}}}
]}.

{plugins, [
  {'lfe-compile', ".*",
    {git, "https://github.com/lfex/rebar3-lfe-compile.git", {branch, "master"}}}
]}.

{provider_hooks, [{pre, [{compile, {lfe, compile}}]}]}.
